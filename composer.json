{
  "name": "solcre/solcre-email-schedule",
  "description": "Base stuff of many internal projects",
  "keywords": [
    "solcre",
    "framework",
    "email",
    "queue",
    "phpmailer"
  ],
  "license": "BSD-3-Clause",
  "require": {
    "php": "^7.4",
    "doctrine/doctrine-orm-module": "^4.0",
    "phpmailer/phpmailer": "^6.5",
    "twig/twig": "^3.0",
    "laminas/laminas-servicemanager": "^3.4",
    "psr/log": "^1.0",
    "smarty/smarty": "^3.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.2",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Solcre\\EmailSchedule\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EmailScheduleTest\\": "./tests/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit"
  }
}
