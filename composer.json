{
  "name": "solcre/solcre-email-schedule",
  "description": "Base stuff of many internal projects",
  "keywords": [
    "solcre",
    "framework",
    "email",
    "queue",
    "phpmailer"
  ],
  "license": "BSD-3-Clause",
  "require": {
    "php": "^8.2",
    "doctrine/doctrine-orm-module": "^6.0",
    "twig/twig": "^3.4",
    "laminas/laminas-servicemanager": "^3.19",
    "smarty/smarty": "^4.2",
    "psr/log": "^2.0 || ^3.0",
    "phpmailer/phpmailer": "^6.6"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^10",
    "friendsofphp/php-cs-fixer": "^3.12"
  },
  "autoload": {
    "psr-4": {
      "Solcre\\EmailSchedule\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EmailScheduleTest\\": "./test/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit"
  }
}
